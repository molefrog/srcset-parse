{
  "name": "srcset-parse",
  "version": "1.0.0",
  "module": "index.js",
  "main": "dist/index.js",
  "size-limit": [
    {
      "path": "index.js",
      "limit": "130 B"
    }
  ],
  "scripts": {
    "test": "uvu -r esm",
    "start": "nodemon --exec \"uvu -r esm\"",
    "build": "rollup -f cjs -d dist index.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@size-limit/preset-small-lib": "4.5.4",
    "core-js": "3.6.5",
    "esm": "3.2.25",
    "nodemon": "1.18.4",
    "rollup": "^2.21.0",
    "size-limit": "4.5.4",
    "uvu": "0.2.1"
  }
}
